    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "2.1.9",
    "cypress": "^13.13.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "^14.2.4",
